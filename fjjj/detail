function showFundQuotes(fundId, tableTitle, tableId){
	var fund_config = {
		dataURL: G_BASE_URL + '/data/sfnew/get_fund_quote_list/' +  fundId,
		tid: tableId,
		cols: [
			{name:'q_nm', display: tableTitle, width:20,nowrap:true},
			{name:'q_price', display:'价格', width:20,nowrap:true},
			{name:'q_amount', display:'挂单(万元)',width:20,nowrap:true}
		],
		rp: 50,
		url_method: 'GET',
		afterShow: updateFundQuoteInfo
	};
	window['fqTable'+fundId] = new SimpleTableBuilder(fund_config);
	window['fqTable'+fundId].show();
}

function updateFundQuoteInfo(data){
	$('table.quotetable tr:nth-child(6n)').css('border-top', '1px solid red');

	if(data.fund_id == fundAId && $("#input_Aprice").val()=='auto'){
		fundAPrice = data.price;
		fundABuy = data.buy;
		fundASell = data.sell;
		var _fundAPrice = data.price;
		if($("input[value='price_type_price']").attr('checked')){
			_fundAPrice = data.price;
		}else if($("input[value='price_type_buy']").attr('checked')){
			_fundAPrice = data.buy;
		}else if($("input[value='price_type_sell']").attr('checked')){
			_fundAPrice = data.sell;
		}
		_fundAPrice = parseFloat(_fundAPrice);
		$('#a_price').val(_fundAPrice.toFixed(3));
	}else if(data.fund_id == fundBId && $("#input_Bprice").val()=='auto'){
		fundBPrice = data.price;
		fundBBuy = data.buy;
		fundBSell = data.sell;
		var _fundBPrice = data.price;
		if($("input[value='price_type_price']").attr('checked')){
			_fundBPrice = data.price;
		}else if($("input[value='price_type_buy']").attr('checked')){
			_fundBPrice = data.buy;
		}else if($("input[value='price_type_sell']").attr('checked')){
			_fundBPrice = data.sell;
		}
		_fundBPrice = parseFloat(_fundBPrice);
		$('#b_price').val(_fundBPrice.toFixed(3));
		calcDisRatio();
	}
}

function updateIndexQuoteInfo(data){
	indexIncrRt = parseFloat(data.rows[0].cell.real_idx_increase_rt);
	//$('#index_incr_rt').val(indexIncrRt.toFixed(2) + '%');
	calcDisRatio();
}

//计算折价
function calcDisRatio(){
	var _estDisRt = estDisRt;
	if(!isActiveFund()){
		$('#asset_ratio').removeAttr('readonly').removeAttr('title');
		var assetRatioVal = parseFloat($('#asset_ratio').val())
		if(isNaN(assetRatioVal)){
			return;
		}
		if('1' == isEstVal){
			_estDisRt = baseFundNetPrice * (assetRatioVal / 100 * indexIncrRt / 100 + 1);
			$('#base_est_val').css('border-color','').attr('title','估值');
		}else{
			_estDisRt = baseFundNetPrice;
			$('#base_est_val').css('border-color','red').attr('title','非估值，当前已是最新净值');
		}
	}else{
		$('#asset_ratio').attr('readonly', true).attr('title','主动基金');
	}

	var _fundAPrice = $('#a_price').val();
	var _fundBPrice = $('#b_price').val();

	$('#base_est_val').val(_estDisRt.toFixed(4));

	var _m_price = (_fundAPrice * fundARatio + _fundBPrice * fundBRatio)/10;
	$('#merge_price').val(_m_price.toFixed(4));
	var _est_dis_rt = (_m_price - _estDisRt) / _estDisRt * 100;
	$('#est_dis_rt').val(_est_dis_rt.toFixed(2));
}

var activeFundIds = ['161818','165310','166011','163406','160127'];

function isActiveFund(){
	return ($.inArray(baseFundId, activeFundIds) >= 0);
}


