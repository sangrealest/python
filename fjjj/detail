var fundChart1 = null;
var fundChart2 = null;

function showChart(){
	fundChart1 = buildChart(fundChart1, $('#chart_select1').val(), 'chart_fund_info1');
	fundChart2 = buildChart(fundChart2, $('#chart_select2').val(), 'chart_fund_info2');
	fundChart1.connect([fundChart2]);
	fundChart2.connect([fundChart1]);
}

function buildChart(chart, chartType, chartId){
	chart = echarts.init(document.getElementById(chartId));
	chart.setOption($.extend({}, defaultChartOptions, {
		title : {
			x: 'center',
			text: __chartTitle[chartType]
		},
		tooltip: {
			trigger: 'axis',
			formatter: function (params) {
				var res = params[0][1];
				res += '<br/>' + params[0][0] + '：' + params[0][2] + __unit[chartType];
				return res;
			}
		},
		yAxis : [
			{
				name:__title[chartType],
				scale: true,
				precision: __precision[chartType],
				splitNumber: __ticks[chartType].splitNumber,
				min:  __ticks[chartType].min,
				max:  __ticks[chartType].max,
				axisLabel : {
					formatter: function(value){
						if('a_amount' == chartType){
							return value + __unit[chartType];
						}else{
							var val = parseFloat(value);
							return val.toFixed(2) + __unit[chartType];
						}
					}
				}
			}
		],
		series : [
			{
				type:'line',
				showAllSymbol:true,
				itemStyle: {
					normal:{color:'#4BB2C5'}
				},
				name:__title[chartType],
				data: __data[chartType]
			}
		]
	}));
	showWatermark(chart, 680, 210);
	return chart;
}

showFundInfo();
showFundQuotes(fundAId, fundAName, 'fund_quote_infoA');
showFundQuotes(fundBId, fundBName, 'fund_quote_infoB');

calcDisRatio();

showChart();

function reloadData(){
	tableFundInfoReload();
	eval('fqTable'+fundAId+'.reload()');
	eval('fqTable'+fundBId+'.reload()');
	//fundStockPositionTable.reload({is_search: 1, fund_id: baseFundId});
}


function showHistTable(){
	var config = {
		dataURL: '/jisiludata/StockFenJiDetail.php?qtype=hist&display=table&fund_id='+baseFundId,
		tid: 'histTable',
		cols: [
			{display: '日期', name:'price_dt', title:'日期', width:40},
			{display: 'A价格', name:'a_price', width:40 },
			{display: 'A涨幅', name:'a_price_increase_rt', width:40,color:true },
			{display: 'A份额(万份)', name:'a_amount', width:40 },
			{display: 'A新增(万份)', title:'A份额变化(万份)', name:'a_amount_increase', width:40,color:true, bgcolor:"yellow" },
			{display: 'A增长', title:'A份额变化率', name:'a_amount_increase_rt', width:40,color:true },
			{display: 'A折价率', name:'a_discount_rt', width:60},
			{display: 'A收益率', name:'a_profit_rt', width:60},
			{display: 'B溢价率', name:'b_discount_rt', width:40 },
			{display: 'B净值杠杆', name:'b_net_leverage_rt', width:40 },
			{display: 'B价格杠杆', name:'b_price_leverage_rt', width:40 },
			{display: '合并溢价', name:'base_discount_rt', width:40, bgcolor:"yellow" },
			{display: '母基净值', name:'net_value', width:40 },
			{display: '母基估值', name:'base_est_val', width:40 },
			{display: '估值误差', name:'est_err', width:40 }
		],
		rp: 50,
		title: '分级历史数据'
	};
	var cfh = new SimpleTableBuilder(config);
	cfh.show();
}

function changeDays(){
	var hist_days = $('#hist_pic_days').val();
	window.location.href = '/data/sfnew/detail/' + baseFundId + '?days=' + hist_days + '#hist_pic';
}

$(function(){
	$("#input_Aprice").bind('change', function(){
		if($("#input_Aprice").val() == 'auto'){
			$('#a_price').attr('readonly', true);
		}else{
			$('#a_price').removeAttr('readonly');
		}
	});

	$("#input_Bprice").bind('change', function(){
		if($("#input_Bprice").val() == 'auto'){
			$('#b_price').attr('readonly', true);
		}else{
			$('#b_price').removeAttr('readonly');
		}
	});

	$("input[name='price_type']").bind('click', function(){
		if($("input[value='price_type_buy']").attr('checked')){
			if($("#input_Aprice").val() == 'auto') $('#a_price').val(fundABuy);
			if($("#input_Bprice").val() == 'auto') $('#b_price').val(fundBBuy);
		}else if($("input[value='price_type_sell']").attr('checked')){
			if($("#input_Aprice").val() == 'auto') $('#a_price').val(fundASell);
			if($("#input_Bprice").val() == 'auto') $('#b_price').val(fundBSell);
		}else{
			if($("#input_Aprice").val() == 'auto') $('#a_price').val(fundAPrice);
			if($("#input_Bprice").val() == 'auto') $('#b_price').val(fundBPrice);
		}
		calcDisRatio();
	});

	$("#a_price").bind('keyup blur', function(){
		calcDisRatio();
	});
	$("#b_price").bind('keyup blur', function(){
		calcDisRatio();
	});
	$("#asset_ratio").bind('keyup blur', function(){
		calcDisRatio();
	});


	showHistTable();
	if($('#tid_fundStockPosition').length > 0){
		showFundStockPosition(baseFundId);
	}
	if($('#tid_fundBondPosition').length > 0){
		showFundBondPosition(baseFundId);
	}
});
</script>

